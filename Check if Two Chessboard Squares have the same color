class Solution {
    public boolean checkTwoChessboards(String coordinate1, String coordinate2) {
        boolean flag1 = ((coordinate1.charAt(0)-'a')+(coordinate1.charAt(1)-'1')) % 2 == 0 ;
        boolean flag2 = ((coordinate2.charAt(0)-'a')+(coordinate2.charAt(1)-'1')) % 2 == 0 ;

        return flag1 == flag2;
    }
}
